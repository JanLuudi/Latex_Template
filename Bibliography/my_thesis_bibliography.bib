@article{Feil2019,
abstract = {In Germany, only approximately 30{\%} by mass of plastics from lightweight packaging waste is recycled; 65{\%} by mass is transferred to inferior residual fractions (sorting residue and mixed plastics), which are currently only utilized thermally. An increase in the recycling of valuable resources in the sense of material recycling would both contribute to the saving of resources and improve the economic situation of plant operators. It is generally known from operating and planning experience that fluctuation in the amount of material loaded into the sorting process is one of the main reasons for suboptimal recycling quotas. In particular, overfilling in the input stream leads to a deterioration of the separation result of the entire process. A novel process control concept envisages equalizing the material flow in such a way that all separation steps are operated in the intended design range. For the example of a lightweight packaging treatment process, the requirements and technological solutions for a sensor-based process control concept will be presented.},
author = {Feil, Alexander and Coskun, Erdogan and Bosling, Marcel and Kaufeld, Sebastian and Pretz, Thomas},
doi = {10.1177/0734242X19826372},
file = {:E$\backslash$:/Jan/Downloads/0734242x19826372.pdf:pdf},
issn = {10963669},
journal = {Waste Management and Research},
keywords = {Sorting,lightweight packaging plants,mechanical–biological waste treatment plants,plastics,process control concept,sensory-based characterization},
number = {2},
pages = {120--126},
pmid = {30678545},
title = {{Improvement of the recycling of plastics in lightweight packaging treatment plants by a process control concept}},
volume = {37},
year = {2019}
}
